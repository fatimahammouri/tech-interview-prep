//I: array
//O: Number

function addNumbers(array){
    //create a result variable
    //iterate over the array adding each value to the result variable
    //return the result 
    var result = 0;
    for(var i = 0; i < array.length; i++){
     result += array[i];   
    }
    
    return result;
}

//addNumbers([1,1,3,4,5])--> 14
